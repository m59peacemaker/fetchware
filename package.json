{
  "name": "fetchware",
  "version": "0.0.0",
  "description": "Middleware for the Fetch API.",
  "main": "build/cjs/index.js",
  "module": "src/index.js",
  "keywords": [
    "fetch",
    "api",
    "middleware",
    "plugin",
    "extend",
